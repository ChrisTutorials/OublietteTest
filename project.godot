[application]

config/name="Game1"
config/description="Testing my first game"
run/main_scene="res://Main.tscn"
run/delta_smoothing=false
boot_splash/image="res://Sprites/ColorTestCoolLrgBoot.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://Sprites/Items/BottleSprite.png"

[autoload]

GameManager="*res://Managers_Singletons/GameManager.tscn"
State="*res://05Resources/Player_Resources/State.gd"
Helpers="*res://Script/Helpers.gd"
List="*res://00U.I./list.tscn"
PauseMenu="*res://00U.I./pause_menu/PauseMenu.tscn"
OnScreenKeyboard="*res://00U.I./OnScreenKeyboard.tscn"
Textbox="*res://00U.I./Textbox.tscn"
SceneChanger="*res://00U.I./SceneChanger/SceneChanger.tscn"
GDict="*res://Managers_Singletons/GDict.gd"
Bgm="*res://06Sounds/BGM/BGM.tscn"
IntegerResolutionHandler="*res://addons/integer_resolution_handler/integer_resolution_handler.gd"
Graveyard="*res://Managers_Singletons/Graveyard.gd"

[display]

window/size/width=256
window/size/height=240
window/size/test_width=1024
window/size/test_height=960
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/AsepriteWizard/plugin.cfg", "res://addons/integer_resolution_handler/plugin.cfg", "res://addons/pixel_ever/plugin.cfg" )

[gui]

theme/custom="res://Assets/MinimalUI/MinimalUI.tres"
theme/custom_font="res://Nokia.tres"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
